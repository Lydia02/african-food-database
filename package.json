{
  "name": "african-food-database",
  "version": "1.0.0",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --coverage --forceExit --detectOpenHandles",
    "seed": "node migrations/seed.js",
    "seed:countries": "node migrations/seedCountries.js",
    "seed:foods": "node migrations/seedFoods.js",
    "seed:nigeria": "node migrations/seedNigerianFoods.js",
    "seed:west": "node migrations/seedWestAfricanFoods.js",
    "seed:east": "node migrations/seedEastAfricanFoods.js",
    "seed:north": "node migrations/seedNorthAfricanFoods.js",
    "seed:southern": "node migrations/seedSouthernAfricanFoods.js",
    "seed:central": "node migrations/seedCentralAfricanFoods.js",
    "seed:combos": "node migrations/seedFoodCombos.js",
    "seed:street": "node migrations/seedStreetFood.js",
    "seed:everyday": "node migrations/seedEverydayFood.js",
    "seed:all-regions": "node -e \"import('./migrations/seedWestAfricanFoods.js').then(m=>m.seedWestAfricanFoods()).then(()=>import('./migrations/seedEastAfricanFoods.js')).then(m=>m.seedEastAfricanFoods()).then(()=>import('./migrations/seedNorthAfricanFoods.js')).then(m=>m.seedNorthAfricanFoods()).then(()=>import('./migrations/seedSouthernAfricanFoods.js')).then(m=>m.seedSouthernAfricanFoods()).then(()=>import('./migrations/seedCentralAfricanFoods.js')).then(m=>m.seedCentralAfricanFoods()).then(()=>import('./migrations/seedFoodCombos.js')).then(m=>m.seedFoodCombos()).then(()=>process.exit(0)).catch(e=>{console.error(e);process.exit(1)})\"",
    "enrich": "node migrations/enrichAll.js",
    "enrich:nutrition": "node migrations/enrichAll.js --nutrition",
    "enrich:wikipedia": "node migrations/enrichAll.js --wikipedia",
    "enrich:discover": "node migrations/enrichAll.js --discover",
    "enrich:dry-run": "node migrations/enrichAll.js --dry-run",
    "discover": "node migrations/importDiscoveries.js",
    "discover:import": "node migrations/importDiscoveries.js --import",
    "discover:import:safe": "node migrations/importDiscoveries.js --import --min-confidence=70 --limit=30"
  },
  "keywords": ["african-food", "recipes", "pantrypal", "firebase"],
  "author": "Lydia",
  "license": "ISC",
  "description": "A comprehensive database of African foods, recipes, and culinary traditions across all 54 countries",
  "dependencies": {
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.14",
    "supertest": "^7.2.2"
  }
}
